# Database
DATABASE_URL=sqlite:./data/bonscompte.db

# JWT Secret (change in production!)
JWT_SECRET=your-secret-key-here

# Server
HOST=0.0.0.0
PORT=8000

# Rate Limiting
# Set to false in development to avoid issues with HMR (Hot Module Reload)
# Set to true in production for security
RATE_LIMIT_ENABLED=true

# Logging
RUST_LOG=info,bonscompte_backend=debug
